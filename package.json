{
  "name": "qiniu-fig-bed",
  "displayName": "qiniu-fig-bed",
  "description": "Upload files to qiniu cloud in VSCode",
  "version": "0.0.9",
  "publisher": "coderfee",
  "icon": "image/upload.png",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "qiniu",
    "七牛",
    "图床"
  ],
  "author": {
    "name": "coderfee",
    "email": "coderfee@outlook.com",
    "url": "http://coderfe.cn"
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=coderfee.qiniu-fig-bed",
  "repository": {
    "url": "https://github.com/coderfe/qiniu-fig-bed.git",
    "type": "git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./extension",
  "contributes": {
    "commands": {
      "command": "extension.qiniu",
      "title": "Upload file to qiniu cloud"
    },
    "keybindings": [
      {
        "command": "extension.qiniu",
        "key": "ctrl+q n",
        "mac": "cmd+q n",
        "linux": "ctrl+q n",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "qiniu config",
      "properties": {
        "qiniu.enable": {
          "type": "boolean",
          "default": false,
          "description": "选择是否开启该插件，true 开启，false 禁用"
        },
        "qiniu.AccessKey": {
          "type": "string",
          "default": "",
          "description": "七牛云 > 个人面板 > 密钥管理 > Access_Key"
        },
        "qiniu.SecretKey": {
          "type": "string",
          "default": "",
          "description": "七牛云 > 个人面板 > 密钥管理 > Secret_Key"
        },
        "qiniu.bucket": {
          "type": "string",
          "default": "",
          "description": "任意一个七牛云空间名称"
        },
        "qiniu.remotePath": {
          "type": "string",
          "default": ""
        },
        "qiniu.domain": {
          "type": "string",
          "default": "",
          "description": "与云空间对应的空间域名"
        },
        "qiniu.protocol": {
          "type": "string",
          "default": "",
          "description": "默认协议，例如 \"https://\""
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "clipboardy": "^0.1.2",
    "qiniu": "^6.1.13"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
